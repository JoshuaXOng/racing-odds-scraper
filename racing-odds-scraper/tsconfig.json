{
  "compilerOptions": {
    "lib": [
      "ESNext",
      "dom"
    ], 
    "outDir": "lib", 
    "removeComments": true, 
    "target": "ES6", 
    "baseUrl": "./", 
    "esModuleInterop": true, 
    "moduleResolution": "node", 
    "paths": {}, 
    "sourceMap": true, 
    "sourceRoot": "/", 
    "alwaysStrict": true, 
    "allowUnreachableCode": false, 
    "noImplicitAny": false, 
    "strictNullChecks": true, 
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true, 
    "noUnusedLocals": true, 
    "noUnusedParameters": true 
  },
  "include": ["./**/*.ts", "src/scrapers/browsers/browser.test.js", "src/scrapers/browsers/browser.test.dtds"],
  "exclude": [
    "node_modules/**/*"
  ]
}